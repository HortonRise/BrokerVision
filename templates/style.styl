///////
///////
/////// Clock Module
///////
///////

.clockWrapper
    width 170px
    height 100px
    display block
    // margin-left 30px
    // margin-top 30px
    position absolute
    left 25%
    //top 6px
    background-color #233646
    overflow hidden

.clock
    width 88px
    height 88px
    display block
    // margin-left 30px
    // margin-top 30px
    position absolute
    left 41px
    top 6px
    background-color #233646
    overflow hidden

.ringWrapper
    width 100%
    height 100%
    display block
    position relative
    // -webkit-clip-path: polygon(0 0, 50% 0, 50% 100%, 0% 100%);
    // clip-path: polygon(0 0, 50% 0, 50% 100%, 0% 100%);


.mask1, .mask2
    width 50%
    height 100%
    display block
    background-color #233646

.mask1
    z-index 10
    position absolute
    left 0
    top 0




.ring1, .ring2
    width 80px
    height 80px
    border 4px white solid
    border-radius 50%
    // transition opacity 0s linear
    position absolute
    left 0
    top 0
    -webkit-clip-path: polygon(0 0, 51% 0, 51% 100%, 0% 100%);
    clip-path: polygon(0 0, 50% 0, 50% 100%, 0% 100%);

.ring1
    transform rotate(0deg)
    opacity 0
    transition opacity 0s linear

.ring2
    transform rotate(180deg)
    opacity 0

.clockDisplay
    position absolute
    width 100%
    height 100%
    top 0
    //margin-top -25px
    text-align center
    z-index 15

.clockNums
    position absolute
    width 100%
    height 50px
    top 50%
    margin-top -24px
    text-align center

.clockNumbers
    color white
    font-size 14px
    font-weight bold

.userInfo
    width 300px
    float right
    position relative

.userText
    width 60%
    text-align right
    float left
    p
        color white
    a
        text-decoration none
        color white

.welcome
    font-size 16px
    margin-top 27px
    margin-bottom 0

.userActions
    font-size 12px
    line-height 0px

.avatar
    float left
    width 40px
    height 40px
    background-image url('http://cdn1.thecomeback.com/wp-content/uploads/2015/12/Star-Wars-countdown-Boyega-Clone-Trooper.jpg')
    margin-top 30px
    background-size cover
    background-position center center
    margin-left 14px



    //AUCTION OVERVIEW

.auctionOverview
    width 100%
    background-color white
    height 320px

.auctionName
    font-weight 300
    font-size 28px
    color black
    line-height 0
    margin-top 42px



///////
///////
/////// Property Lines Module
///////
///////

.properties
    width 65%
    //margin 0px auto
    margin-top 20px
    display inline-block
    position relative
    height 200px
    z-index 2
    //background-color .ddddff

.property
    width 100%
    margin-bottom 10px
    display block
    position relative
    height 58px

.propertyName
    font-size 14px
    font-weight 300

.propertyLine
    // width 30%
    height 20px
    position absolute
    top 50%
    margin-top -3px
    // opacity .6
    border-radius 3px 0 0 3px
    transition width 1s ease-in-out

.propertyLine1
    background-color #14A6DA
    // width 100%

.propertyLine2
    background-color #46D6C7

.propertyLine3
    background-color #99CE56


.propertyImgWrapper
    display block
    width 50px
    height 50px
    border-radius 50%
    background-color white
    position absolute
    left 0%
    top 50%
    margin-top -18px
    margin-left -2px
    transition left 1s ease-in-out
    -webkit-box-shadow: 0px 5px 12px -1px rgba(0,0,0,0.2);
    -moz-box-shadow: 0px 5px 12px -1px rgba(0,0,0,0.2);
    box-shadow: 0px 5px 12px -1px rgba(0,0,0,0.2);

// .propertyImgWrapper1
    // left 100%

.propertyValueWrapper
    display block
    width 100px
    height 36px
    //border-radius 50%
    background-color white
    position absolute
    left 0%
    top 50%
    margin-top -11px
    margin-left -94px
    transition left 1s ease-in-out
    text-align center
    -webkit-box-shadow: 0px 4px 14px -1px rgba(0,0,0,0.2);
    -moz-box-shadow: 0px 4px 14px -1px rgba(0,0,0,0.2);
    box-shadow: 0px 4px 14px -1px rgba(0,0,0,0.2);

// .propertyValueWrapper1
//     left 100%

.propertyValue
    font-size 13px
    font-weight 700
    line-height 0
    margin-top 17px

.propertyValue1
    color #14A6DA

.propertyValue2
    color #46D6C7

.propertyValue3
    color #99CE56

.propertyImg
    display block
    width 46px
    height 46px
    border-radius 50%
    //background-color red
    margin-left 2px
    margin-top 2px

.propertyImg1
    background-image url('http://www.chicagoarchitecture.info/Images/TheLoop/1SouthWacker-003.jpg')
    background-size cover

.propertyImg2
    background-image url('http://www3.jjc.edu/ftp/wdc13/rturza/pictures/Sears_Tower.jpg')
    background-size cover

.propertyImg3
    background-image url('https://media-cdn.tripadvisor.com/media/photo-p/0a/e4/e6/12/exterior-night.jpg')
    background-size cover


.finishLine
    width 0px
    border-right 1px dashed #888888
    height 112%
    position absolute
    left 67%
    top 0
    z-index 0

    //Preferences

.preferences
    width 27%
    display inline-block
    //background-color #440022
    float right
    margin-top 0px
    padding-left 12px

.clientLogo
    width 70%
