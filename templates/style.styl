.factNum
    margin-top 16px
    font-size 21px
    font-weight 400




///////bar graphs

.bars
    background-color #F5FAFE
    width 100%
    height 200px
    display block
    margin 0px auto
    margin-top 70px
    position relative
    margin-bottom 60px

.var
    width 16%
    display inline-block
    height 100%
    position relative
    margin-right 3%
    //padding-right 12px

.varNameDiv
    width 100%
    background-color #F5FAFE
    height 40px
    padding-left 25px
    position absolute
    top 0

.varName
    color #555555
    font-size 13px
    width 60%
    line-height 13px

.graph
    display block
    background-color #F5FAFE
    width 50%
    height 150px
    position absolute
    bottom 0
    right 0

.barsWrapper
    width 100%
    position absolute
    height 120px
    bottom 32px

.bar
    width 32%
    display inline-block
    float left
    position absolute
    bottom 0
    min-height 25px
    z-index 2
    transition height .5s ease-in-out
    //padding-top 10px

.bar1
    background-color #14A6DA
    height 0%
    left 0

.bar2
    background-color #46D6C7
    height 0%
    left 33%

.bar3
    background-color #99CE56
    height 0%
    left 66%

.barVal
    transform rotate(-90deg)
    color white
    position relative
    text-align right
    margin-top 12px
    font-size 13px
    // margin-right 13px

.pref
    width 100%
    height 22px
    background-color #555555
    position absolute
    bottom 0
    border-radius 3px
    text-align center

.prefVal
    margin 0
    position relative
    color white
    margin-top 1px
    font-size 14px

.graphHR
    width 100%
    color #777777
    position absolute
    z-index 0
    opacity .2

.graphHR1
    top 150px

.graphHR2
    top 125px

.graphHR3
    top 100px

.graphHR4
    top 75px

.graphHR5
    top 50px

