body
    // background-color #0061AF
    background-color #F5FAFE
    font-family 'Open Sans', sans-serif
    padding 0
    margin 0

.container
    width 1020px
    position relative
    margin 0px auto

.clearfix
    width 100%
    height 1px
    clear both

a
    text-decoration none


.sideNav
    width 100px
    background-color black
    height 100%
    position fixed
    left 0
    top 0
    z-index 100


